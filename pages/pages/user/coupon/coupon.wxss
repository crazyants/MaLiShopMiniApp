.ui-tab {
  border-bottom: 2rpx solid #f5f5f5;
}
.coupon-new-box {
  border-bottom: 2rpx solid #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-bottom: 16rpx;
}
.coupon-new-input {
  width: 85%;
  border: 2rpx solid #f5f5f5;
  height: 70rpx;
  border-radius: 4rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.coupon-new-input input {
  text-align: left;
  width: 90%;
  margin: 0 auto;
  font-size: 28rpx;
  color: #27323f;
}
.coupon-new-button {
  width: 160rpx;
  color: #27323f;
  background-color: #ffd305;
  height: 68rpx;
  line-height: 68rpx;
  text-align: center;
  border-radius: 4rpx;
  font-size: 30rpx;
}
.coupon-list-box {
  padding-top: 50rpx;
  padding-bottom: 100rpx;
}
.coupon-list-box .coupon-list {
  background-color: #ffd305;
  width: 90%;
  margin: 0 auto;
  color: #27323f;
  border-radius: 6rpx;
  margin-bottom: 40rpx;
  box-shadow: 0 6rpx 18rpx #ba9b04;
}
.coupon-list-box .coupon-list-price {
  font-size: 50rpx;
}
.coupon-list-box .coupon-list-price text {
  font-size: 26rpx;
  margin-left: -12rpx;
}
.coupon-list-box .coupon-list-info {
  font-size: 34rpx;
  font-weight: 600;
}
.coupon-list-box .coupon-list-time {
  font-size: 24rpx;
  opacity: .3;
  padding-top: 10rpx;
}
.coupon-list-text {
  width: 100%;
  height: 80rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: repeat-x;
}
.coupon-list-text text {
  width: 100%;
  height: 80rpx;
  max-height: 80rpx;
  display: flex;
  align-items: center;
  padding-left: 40rpx;
  padding-right: 40rpx;
  padding-top: 10rpx;
  font-size: 26rpx;
}
