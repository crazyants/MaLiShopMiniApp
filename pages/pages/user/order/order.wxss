page {
  background-color: #f5f5f5;
}
.order-content-box swiper-item {
  overflow-y: scroll;
}
.order-header-box {
  height: 90rpx;
  background-color: #fff;
}
.ui-tabs {
  height: 90rpx;
  border-bottom: 2rpx solid #f5f5f5;
}
.order-list {
  color: #27323f;
  background-color: #ffffff;
}
.order-ui-row {
  border-bottom: 2rpx solid #f5f5f5;
}
.order-one-img {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.order-one-img image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 4rpx;
}
.order-goods-name {
  width: 95%;
  font-size: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.order-goods-info {
  width: 95%;
  padding-top: 12rpx;
  color: #7b8196;
  font-size: 26rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.order-button-time {
  display: flex;
  width: 170rpx;
  height: 60rpx;
  background-color: #ffd305;
  color: #27323f;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  border-radius: 4rpx;
}
.no-more-goods {
  width: 100%;
  height: 500rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.no-order-img {
  width: 200rpx;
  height: 200rpx;
}
.no-more-goods .text {
  font-size: 26rpx;
  color: #bfbfbf;
}
